]]
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Nhập chiều khoá", HidePremium = true, SaveConfig = true, ConfigFolder = "OrionTest"})

OrionLib:MakeNotification({
	Name = "Cần chiều khoá",
	Content = "Nhập mẹ mày chiều khoá đê",
	Image = "rbxassetid://4483345998",
	Time = 10
})

_G.Key = "kibaxkabi" -- You Put Your Key Here
_G.KeyInput = "string"

function MakeScriptHub()
loadstring(game:HttpGet(("https://raw.githubusercontent.com/Kietba/Kietba/main/KiBa%20script"), true))() -- You Put Your Script Here

end


function CorrectKeyNotifications()
    OrionLib:MakeNotification({
        Name = "Đúng",
        Content = "Đúng chiều khoá rồi thằng lồn",
        Image = "rbxassetid://4483345998",
        Time = 10
    })
end

function WrongKeyNotifications()
    OrionLib:MakeNotification({
        Name = "Sai",
        Content = "Sai con mẹ mày chiều khoá rồi",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

local Tab = Window:MakeTab({
	Name = "Chiều khoá",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddTextbox({
	Name = "Nhập chiều khoá 👉",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
        _G.KeyInput = Value
	end	  
})

Tab:AddButton({
	Name = "Xác nhận",
	Callback = function()
        if _G.KeyInput == _G.Key then
         MakeScriptHub()
         CorrectKeyNotifications()
     else
        WrongKeyNotifications()
        end
  	end    
})

