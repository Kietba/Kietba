local Button = Tab:CreateButton({
   Name = "Respawn",
   Callback = function()
   -- LocalScript ƒë·∫∑t trong StarterGui ho·∫∑c StarterPlayerScripts

-- T·∫°o GUI
local player = game.Players.LocalPlayer

local screenGui = Instance.new("ScreenGui")
screenGui.Name = "RespawnGui"
screenGui.ResetOnSpawn = false
screenGui.Parent = player:WaitForChild("PlayerGui")

local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 120, 0, 50) -- GUI nh·ªè h∆°n
frame.Position = UDim2.new(0.5, -60, 0.8, -25) -- cƒÉn gi·ªØa d∆∞·ªõi m√†n h√¨nh
frame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
frame.Parent = screenGui
-- Kh√¥ng c·∫ßn Active/Draggable v√¨ b·∫°n y√™u c·∫ßu xo√° Drag

local button = Instance.new("TextButton")
button.Size = UDim2.new(1, 0, 1, 0)
button.Text = "Respawn"
button.TextScaled = true
button.BackgroundColor3 = Color3.fromRGB(100, 100, 255)
button.Parent = frame
button.Active = true
button.Draggable = true
-- X·ª≠ l√Ω respawn
local deathPosition = nil

button.MouseButton1Click:Connect(function()
    local character = player.Character
    if character and character:FindFirstChild("HumanoidRootPart") then
        -- L∆∞u v·ªã tr√≠ tr∆∞·ªõc khi ch·∫øt
        deathPosition = character.HumanoidRootPart.Position

        -- Gi·∫øt nh√¢n v·∫≠t
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.Health = 0
        end
    end
end)

player.CharacterAdded:Connect(function(newCharacter)
    if deathPosition then
        -- Khi nh√¢n v·∫≠t respawn, d·ªãch chuy·ªÉn t·ªõi v·ªã tr√≠ ƒë√£ l∆∞u
        local hrp = newCharacter:WaitForChild("HumanoidRootPart")
        hrp.CFrame = CFrame.new(deathPosition)

        -- X√≥a v·ªã tr√≠ ƒë·ªÉ tr√°nh l·ªói d·ªãch chuy·ªÉn li√™n t·ª•c
        deathPosition = nil
    end
end)
   end,
})

local Button = Tab:CreateButton({
   Name = "Rejoin",
   Callback = function()
   -- RejoinScript.lua
local TeleportService = game:GetService("TeleportService")
local Players = game:GetService("Players")

local placeId = game.PlaceId
local player = Players.LocalPlayer

-- H√†m Rejoin
local function rejoinGame()
	if player then
		TeleportService:Teleport(placeId, player)
	end
end

-- G·ªçi h√†m Rejoin khi c·∫ßn (VD: g√°n v√†o n√∫t)
rejoinGame()
   end,
})

