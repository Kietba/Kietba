-- T·∫°o ScrollingFrame ƒë·ªÉ cu·ªôn danh s√°ch ng∆∞·ªùi ch∆°i
local playerListFrame = Instance.new("ScrollingFrame")
playerListFrame.Size = UDim2.new(0, 200, 0, 200)  -- K√≠ch th∆∞·ªõc c·ªßa ScrollingFrame
playerListFrame.Position = UDim2.new(1, -220, 0, 5)  -- D√≠nh li·ªÅn b√™n ph·∫£i n√∫t ">"
playerListFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
playerListFrame.Visible = false  -- Ban ƒë·∫ßu ·∫©n danh s√°ch ng∆∞·ªùi ch∆°i
playerListFrame.Parent = screenGui
playerListFrame.CanvasSize = UDim2.new(0, 0, 0, 0)  -- ƒê·∫∑t chi·ªÅu cao c·ªßa canvas (s·∫Ω thay ƒë·ªïi khi th√™m c√°c n√∫t)

local playerListLayout = Instance.new("UIListLayout")
playerListLayout.Parent = playerListFrame
playerListLayout.Padding = UDim.new(0, 5)
playerListFrame.ScrollBarThickness = 10  -- ƒê·ªô d√†y thanh cu·ªôn

-- Hi·ªÉn th·ªã danh s√°ch ng∆∞·ªùi ch∆°i trong server
local function showPlayerList()
    -- X√≥a danh s√°ch c≈© tr∆∞·ªõc khi hi·ªÉn th·ªã l·∫°i
    for _, child in pairs(playerListFrame:GetChildren()) do
        if child:IsA("TextButton") then
            child:Destroy()
        end
    end

    -- L·∫•y danh s√°ch ng∆∞·ªùi ch∆°i
    for _, player in pairs(game.Players:GetPlayers()) do
        local playerButton = Instance.new("TextButton")
        playerButton.Size = UDim2.new(0, 180, 0, 30)
        playerButton.Text = player.DisplayName  -- Hi·ªÉn th·ªã t√™n gi·∫£
        playerButton.TextColor3 = Color3.fromRGB(0, 0, 0)
        playerButton.BackgroundColor3 = Color3.fromRGB(220, 220, 220)
        playerButton.Font = Enum.Font.Gotham
        playerButton.TextSize = 14
        playerButton.Parent = playerListFrame

        -- Khi nh·∫•n v√†o t√™n gi·∫£, ƒëi·ªÅn t√™n th·∫≠t v√†o TextBox
        playerButton.MouseButton1Click:Connect(function()
            textBox.Text = player.Name  -- ƒêi·ªÅn t√™n th·∫≠t v√†o TextBox
            playerListFrame.Visible = false  -- ·∫®n danh s√°ch ng∆∞·ªùi ch∆°i sau khi ch·ªçn
            toggleButton.Text = ">"  -- Chuy·ªÉn n√∫t v·ªÅ tr·∫°ng th√°i ">"
        end)
    end

    -- C·∫≠p nh·∫≠t l·∫°i CanvasSize c·ªßa ScrollingFrame ƒë·ªÉ ph√π h·ª£p v·ªõi s·ªë l∆∞·ª£ng ng∆∞·ªùi ch∆°i
    playerListFrame.CanvasSize = UDim2.new(0, 0, 0, playerListFrame.UIListLayout.AbsoluteContentSize.Y)
end

-- Bi·∫øn ƒë·ªÉ theo d√µi camera
local cameraToggled = false
local currentTargetPlayer = nil

-- X·ª≠ l√Ω s·ª± ki·ªán khi nh·∫•n n√∫t Toggle
button.MouseButton1Click:Connect(function()
    local inputLetter = textBox.Text:sub(1, 1):upper()  -- L·∫•y ch·ªØ c√°i ƒë·∫ßu v√† chuy·ªÉn th√†nh ch·ªØ hoa
    if inputLetter and inputLetter ~= "" then
        local foundPlayers = {}  -- Danh s√°ch c√°c ng∆∞·ªùi ch∆°i t√¨m ƒë∆∞·ª£c
        for _, player in pairs(game.Players:GetPlayers()) do
            if player.Name:sub(1, 1):upper() == inputLetter then
                table.insert(foundPlayers, player)  -- Th√™m ng∆∞·ªùi ch∆°i v√†o danh s√°ch
            end
        end
        
        if #foundPlayers > 0 then
            -- Ch·ªçn ng∆∞·ªùi ch∆°i ƒë·∫ßu ti√™n trong danh s√°ch t√¨m th·∫•y
            local targetPlayer = foundPlayers[1]

            -- N·∫øu camera ƒëang ·ªü ch·∫ø ƒë·ªô theo d√µi v√† ng∆∞·ªùi ch∆°i hi·ªán t·∫°i ƒë√£ ƒë∆∞·ª£c ch·ªçn
            if cameraToggled then
                local camera = game.Workspace.CurrentCamera
                camera.CameraType = Enum.CameraType.Custom  -- ƒê·∫∑t l·∫°i camera v·ªÅ ch·∫ø ƒë·ªô m·∫∑c ƒë·ªãnh (cho ph√©p xoay)
                cameraToggled = false
            else
                -- N·∫øu camera ch∆∞a b·∫≠t theo d√µi, b·∫≠t camera theo d√µi ng∆∞·ªùi ch∆°i
                local targetPlayerChar = targetPlayer.Character
                if targetPlayerChar and targetPlayerChar:FindFirstChild("HumanoidRootPart") then
                    local camera = game.Workspace.CurrentCamera
                    camera.CameraType = Enum.CameraType.Scriptable  -- Ch·∫ø ƒë·ªô camera c·ªë ƒë·ªãnh
                    cameraToggled = true  -- ƒê√°nh d·∫•u l√† camera ƒë√£ b·∫≠t theo d√µi
                    currentTargetPlayer = targetPlayer  -- L∆∞u ng∆∞·ªùi ch∆°i hi·ªán t·∫°i
                end
            end
        else
            -- N·∫øu kh√¥ng t√¨m th·∫•y ng∆∞·ªùi ch∆°i n√†o, hi·ªÉn th·ªã th√¥ng b√°o l·ªói
            local errorLabel = Instance.new("TextLabel")
            errorLabel.Size = UDim2.new(0, 250, 0, 30)
            errorLabel.Position = UDim2.new(0, 25, 0, 120)
            errorLabel.Text = "Kh√¥ng t√¨m th·∫•y ng∆∞·ªùi ch∆°i!"
            errorLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
            errorLabel.BackgroundTransparency = 1
            errorLabel.Font = Enum.Font.Gotham
            errorLabel.TextSize = 14
            errorLabel.Parent = frame

            -- L√†m cho th√¥ng b√°o l·ªói bi·∫øn m·∫•t sau 5 gi√¢y
            delay(5, function()
                errorLabel:Destroy()  -- X√≥a th√¥ng b√°o l·ªói sau 5 gi√¢y
            end)
        end
    else
        -- N·∫øu kh√¥ng nh·∫≠p ch·ªØ c√°i, hi·ªÉn th·ªã th√¥ng b√°o l·ªói
        local errorLabel = Instance.new("TextLabel")
        errorLabel.Size = UDim2.new(0, 250, 0, 30)
        errorLabel.Position = UDim2.new(0, 25, 0, 120)
        errorLabel.Text = "Vui l√≤ng nh·∫≠p ch·ªØ c√°i ƒë·∫ßu!"
        errorLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
        errorLabel.BackgroundTransparency = 1
        errorLabel.Font = Enum.Font.Gotham
        errorLabel.TextSize = 14
        errorLabel.Parent = frame

        -- L√†m cho th√¥ng b√°o l·ªói bi·∫øn m·∫•t sau 5 gi√¢y
        delay(5, function()
            errorLabel:Destroy()  -- X√≥a th√¥ng b√°o l·ªói sau 5 gi√¢y
        end)
    end
end)

-- X·ª≠ l√Ω s·ª± ki·ªán khi nh·∫•n v√†o n√∫t ">"
toggleButton.MouseButton1Click:Connect(function()
    -- N·∫øu danh s√°ch ch∆∞a hi·ªÉn th·ªã, hi·ªÉn th·ªã danh s√°ch ng∆∞·ªùi ch∆°i v√† chuy·ªÉn n√∫t th√†nh "<"
    if not playerListFrame.Visible then
        playerListFrame.Visible = true
        toggleButton.Text = "<"  -- Chuy·ªÉn n√∫t th√†nh "<"
        showPlayerList()  -- Hi·ªÉn th·ªã danh s√°ch ng∆∞·ªùi ch∆°i trong server
    else
        playerListFrame.Visible = false  -- ·∫®n danh s√°ch ng∆∞·ªùi ch∆°i
        toggleButton.Text = ">"  -- Chuy·ªÉn n√∫t tr·ªü l·∫°i ">"
    end
end)

-- Theo d√µi chuy·ªÉn ƒë·ªông c·ªßa ng∆∞·ªùi ch∆°i n·∫øu camera ƒëang ·ªü ch·∫ø ƒë·ªô theo d√µi
game:GetService("RunService").Heartbeat:Connect(function()
    if cameraToggled and currentTargetPlayer then
        local camera = game.Workspace.CurrentCamera
        local targetPlayerChar = currentTargetPlayer.Character
        if targetPlayerChar and targetPlayerChar:FindFirstChild("HumanoidRootPart") then
            -- C·∫≠p nh·∫≠t v·ªã tr√≠ camera theo ng∆∞·ªùi ch∆°i
            camera.CFrame = CFrame.new(targetPlayerChar.HumanoidRootPart.Position + Vector3.new(0, 5, 10), targetPlayerChar.HumanoidRootPart.Position)
        end
    end
end)
   end,
})

