-- N√∫t toggle danh s√°ch
ToggleListButton.MouseButton1Click:Connect(function()
	SavedListFrame.Visible = not SavedListFrame.Visible
	ToggleListButton.Text = SavedListFrame.Visible and "<" or ">"
end)

-- Tween t·ªõi ng∆∞·ªùi ch∆°i
Button.MouseButton1Click:Connect(function()
	local name = TextBox.Text
	for _, player in ipairs(Players:GetPlayers()) do
		if player ~= LocalPlayer and string.lower(player.Name):find(string.lower(name)) then
			local targetHRP = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
			if targetHRP then
				local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
				local myHRP = character:WaitForChild("HumanoidRootPart")
				local tween = TweenService:Create(myHRP, TweenInfo.new(1), {CFrame = targetHRP.CFrame * CFrame.new(0, 0, -3)})
				tween:Play()
				showStatusMessage("ƒê√£ t·ªõi " .. player.Name)
				return
			end
		end
	end
	showStatusMessage("Kh√¥ng t√¨m th·∫•y ng∆∞·ªùi ch∆°i.")
end)
   end,
})

local Button = Tab:CreateButton({
   Name = "Esp Team",
   Callback = function()
   -- T·∫°o ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
screenGui.Parent = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")
screenGui.ResetOnSpawn = false

-- T·∫°o Toggle Button
local toggleButton = Instance.new("TextButton")
toggleButton.Size = UDim2.new(0, 50, 0, 50)
toggleButton.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
toggleButton.Text = "ESP OFF"
toggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
toggleButton.Parent = screenGui
toggleButton.Active = true
toggleButton.Draggable = true

-- T·∫°o label g√≥c tr√°i hi·ªÉn th·ªã team
local teamLabel = Instance.new("TextLabel")
teamLabel.Size = UDim2.new(0, 200, 0, 30)
teamLabel.Position = UDim2.new(0, 10, 0, 10)
teamLabel.BackgroundTransparency = 1
teamLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
teamLabel.TextScaled = true
teamLabel.Text = ""
teamLabel.Parent = screenGui

-- Tr·∫°ng th√°i b·∫≠t/t·∫Øt
local espEnabled = false
local highlights = {}
local teamColors = {}

-- H√†m t·∫°o c·∫ßu v·ªìng
local hue = 0
game:GetService("RunService").RenderStepped:Connect(function()
    hue = (hue + 1) % 360
    local color = Color3.fromHSV(hue/360, 1, 1)
    if espEnabled then
        teamLabel.TextColor3 = color
    end
end)

-- C·∫≠p nh·∫≠t danh s√°ch m√†u team
local function updateTeamColors()
    for _, team in pairs(game:GetService("Teams"):GetTeams()) do
        teamColors[team.Name] = team.TeamColor.Color
    end
end

updateTeamColors()

game:GetService("Teams").ChildAdded:Connect(updateTeamColors)
game:GetService("Teams").ChildRemoved:Connect(updateTeamColors)

-- H√†m t·∫°o ESP
local function addESP(player)
    if player ~= game.Players.LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        if player.Character:FindFirstChild("ESP_Billboard") then return end

        local teamColor = teamColors[player.Team.Name] or Color3.fromRGB(255, 255, 255)

        local highlight = Instance.new("Highlight")
        highlight.FillColor = teamColor
        highlight.OutlineColor = teamColor
        highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
        highlight.Parent = player.Character
        highlights[player] = highlight

        local billboard = Instance.new("BillboardGui")
        billboard.Name = "ESP_Billboard"
        billboard.Adornee = player.Character:WaitForChild("Head")
        billboard.Size = UDim2.new(0, 100, 0, 50)
        billboard.StudsOffset = Vector3.new(0, 2, 0)
        billboard.AlwaysOnTop = true
        billboard.Parent = player.Character

        local textLabel = Instance.new("TextLabel")
        textLabel.Size = UDim2.new(1, 0, 1, 0)
        textLabel.BackgroundTransparency = 1
        textLabel.TextColor3 = teamColor
        textLabel.TextScaled = true
        textLabel.Text = player.Name .. " | " .. tostring(player.Team) .. " | HP: " .. math.floor(player.Character.Humanoid.Health)
        textLabel.Parent = billboard

        local selectionBox = Instance.new("SelectionBox")
        selectionBox.Adornee = player.Character
        selectionBox.LineThickness = 0.05
        selectionBox.Color3 = Color3.fromRGB(170, 0, 255)
        selectionBox.SurfaceTransparency = 1
        selectionBox.Parent = player.Character
    end
end

-- H√†m x√≥a ESP
local function removeESP()
    for _, player in pairs(game.Players:GetPlayers()) do
        if player.Character then
            local billboard = player.Character:FindFirstChild("ESP_Billboard")
            if billboard then billboard:Destroy() end

            local selectionBox = player.Character:FindFirstChildOfClass("SelectionBox")
            if selectionBox then selectionBox:Destroy() end

            if highlights[player] then
                highlights[player]:Destroy()
                highlights[player] = nil
            end
        end
    end
end

-- L√†m tr√≤n g√≥c
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 12)
corner.Parent = toggleButton

-- Vi·ªÅn n·ªÅn m√†u t√≠m (vi·ªÅn n√∫t, kh√¥ng ph·∫£i ch·ªØ)
local stroke = Instance.new("UIStroke")
stroke.Color = Color3.fromRGB(170, 0, 255) -- M√†u t√≠m
stroke.Thickness = 2 -- ƒê·ªô d√†y vi·ªÅn
stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border -- Vi·ªÅn bao n√∫t
stroke.Parent = toggleButton

-- Toggle
toggleButton.MouseButton1Click:Connect(function()
    espEnabled = not espEnabled
    toggleButton.Text = espEnabled and "ESP ON" or "ESP OFF"

    local localPlayer = game.Players.LocalPlayer
    local myTeam = localPlayer.Team and localPlayer.Team.Name or "Kh√¥ng c√≥ team"

    if espEnabled then
        teamLabel.Text = "T√¥i team: " .. myTeam
    else
        teamLabel.Text = ""
        removeESP()
    end
end)

-- Lu√¥n ch·∫°y update ESP
game:GetService("RunService").RenderStepped:Connect(function()
    if espEnabled then
        updateTeamColors()
        local localPlayer = game.Players.LocalPlayer
        local myTeam = localPlayer.Team and localPlayer.Team.Name or "Kh√¥ng c√≥ team"
        teamLabel.Text = "T√¥i team: " .. myTeam

        for _, player in pairs(game.Players:GetPlayers()) do
            if player ~= localPlayer and player.Character and player.Character:FindFirstChild("Humanoid") then
                addESP(player)
                local billboard = player.Character:FindFirstChild("ESP_Billboard")
                local hp = math.floor(player.Character.Humanoid.Health)
                local teamColor = teamColors[player.Team.Name] or Color3.fromRGB(255, 255, 255)

                if billboard then
                    local label = billboard:FindFirstChildOfClass("TextLabel")
                    if label then
                        label.Text = player.Name .. " | " .. tostring(player.Team) .. " | HP: " .. hp
                        label.TextColor3 = teamColor
                    end
                end

                if highlights[player] then
                    highlights[player].FillColor = teamColor
                    highlights[player].OutlineColor = teamColor
                end

                local box = player.Character:FindFirstChildOfClass("SelectionBox")
                if box then
                    box.Color3 = Color3.fromRGB(170, 0, 255)
                end
            end
        end
    end
end)
   end,
})

local Button = Tab:CreateButton({
   Name = "Esp player",
   Callback = function()
   local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- Function to add ESP to a player
local function addESP(player)
	if player == LocalPlayer then return end

	local function onCharacterAdded(character)
		local highlight = Instance.new("Highlight")
		highlight.Name = "ESP_Highlight"
		highlight.FillColor = Color3.fromRGB(0, 0, 0) -- Blue
		highlight.OutlineColor = Color3.fromRGB(0, 0, 1)
		highlight.OutlineTransparency = 0
		highlight.FillTransparency = 1
		highlight.Adornee = character
		highlight.Parent = character

		local head = character:WaitForChild("Head", 5)
		if head then
			local billboard = Instance.new("BillboardGui")
			billboard.Name = "ESP_Name"
			billboard.Size = UDim2.new(0, 100, 0, 30)
			billboard.StudsOffset = Vector3.new(0, 2, 0)
			billboard.AlwaysOnTop = true
			billboard.Adornee = head
			billboard.Parent = head

			local nameLabel = Instance.new("TextLabel")
			nameLabel.Size = UDim2.new(1, 0, 1, 0)
			nameLabel.BackgroundTransparency = 1
			nameLabel.Text = player.Name
			nameLabel.TextColor3 = Color3.fromRGB(0, 170, 255)
			nameLabel.TextStrokeTransparency = 0.5
			nameLabel.TextScaled = true
			nameLabel.Font = Enum.Font.SourceSansBold
			nameLabel.Parent = billboard
		end
	end

	-- Handle character spawn
	if player.Character then
		onCharacterAdded(player.Character)
	end
	player.CharacterAdded:Connect(onCharacterAdded)
end

-- Add ESP to all players
for _, player in pairs(Players:GetPlayers()) do
	addESP(player)
end

-- Add ESP to new players
Players.PlayerAdded:Connect(function(player)
	addESP(player)
end)
   end,
})

