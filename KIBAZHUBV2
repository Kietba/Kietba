-- Npc

local Tab = Window:CreateTab("Npc", 4483362458) -- Title, Image

local Section = Tab:CreateSection("Script")

local Button = Tab:CreateButton({
   Name = "Hitbox Npc Near",
   Callback = function()
 local Players = game:GetService("Players")
local localPlayer = Players.LocalPlayer

-- Create GUI
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "HitboxGui"
screenGui.ResetOnSpawn = false
screenGui.Parent = localPlayer:WaitForChild("PlayerGui")

-- Create Toggle Button
local toggleButton = Instance.new("TextButton")
toggleButton.Name = "ToggleButton"
toggleButton.Size = UDim2.new(0.2, 0, 0.08, 0)
toggleButton.Position = UDim2.new(0.4, 0, 0.85, 0)
toggleButton.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
toggleButton.Text = "Hitbox OFF"
toggleButton.Font = Enum.Font.SourceSansBold
toggleButton.TextSize = 24
toggleButton.Draggable = true
toggleButton.Active = true
toggleButton.Selectable = true
toggleButton.Parent = screenGui

-- L√†m tr√≤n g√≥c
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 12)
corner.Parent = toggleButton

-- Vi·ªÅn n·ªÅn m√†u t√≠m (vi·ªÅn n√∫t, kh√¥ng ph·∫£i ch·ªØ)
local stroke = Instance.new("UIStroke")
stroke.Color = Color3.fromRGB(170, 0, 255) -- M√†u t√≠m
stroke.Thickness = 2 -- ƒê·ªô d√†y vi·ªÅn
stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border -- Vi·ªÅn bao n√∫t
stroke.Parent = toggleButton

-- Hitbox Logic
local isOn = false
local defaultSize = Vector3.new(2, 2, 1)
local hitboxSize = Vector3.new(15, 15, 15)

local function updateHitboxes(size)
	for _, model in ipairs(workspace:GetDescendants()) do
		if model:IsA("Model") 
			and model:FindFirstChild("Humanoid") 
			and model:FindFirstChild("HumanoidRootPart") 
			and model ~= localPlayer.Character then

			local hrp = model.HumanoidRootPart
			hrp.Size = size
			hrp.Transparency = 0.5
			hrp.CanCollide = false
		end
	end
end

-- Toggle behavior
toggleButton.MouseButton1Click:Connect(function()
	isOn = not isOn
	if isOn then
		toggleButton.Text = "Hitbox ON"
		toggleButton.BackgroundColor3 = Color3.fromRGB(100, 255, 100)
		updateHitboxes(hitboxSize)
	else
		toggleButton.Text = "Hitbox OFF"
		toggleButton.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
		updateHitboxes(defaultSize)
	end
end)
   end,
})

local Button = Tab:CreateButton({
   Name = "Kill Npc Near",
   Callback = function()
   -- L·∫•y ng∆∞·ªùi ch∆°i v√† mouse
local player = game.Players.LocalPlayer
local mouse = player:GetMouse()

-- T·∫°o GUI
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = player:WaitForChild("PlayerGui")
screenGui.ResetOnSpawn = false

-- T·∫°o Frame ch√≠nh c·ªßa GUI (k√≠ch th∆∞·ªõc nh·ªè h∆°n)
local mainFrame = Instance.new("Frame", screenGui)
mainFrame.Size = UDim2.new(0, 75, 0, 75)  -- K√≠ch th∆∞·ªõc nh·ªè h∆°n
mainFrame.Position = UDim2.new(0, 50, 0, 50)  -- ƒê·∫∑t v√†o gi·ªØa m√†n h√¨nh
mainFrame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)

-- T·∫°o n√∫t "Kill Nearby NPCs"
local killBtn = Instance.new("TextButton", mainFrame)
killBtn.Size = UDim2.new(1, 0, 1, 0)
killBtn.Text = "Kill NPCs"
killBtn.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
killBtn.TextColor3 = Color3.new(1, 1, 1)
killBtn.Font = Enum.Font.SourceSansBold
killBtn.TextSize = 14  -- K√≠ch th∆∞·ªõc ch·ªØ nh·ªè h∆°n
killBtn.TextWrapped = true  -- Cho ph√©p ch·ªØ xu·ªëng d√≤ng n·∫øu c·∫ßn
killBtn.Active = true
killBtn.Draggable = true  -- Cho ph√©p k√©o th·∫£

-- H√†m gi·∫øt NPC g·∫ßn ng∆∞·ªùi ch∆°i
local function killNearbyNPCs()
    local character = player.Character
    if not character then return end

    local root = character:FindFirstChild("HumanoidRootPart")
    if not root then return end

    -- Duy·ªát qua t·∫•t c·∫£ c√°c ƒë·ªëi t∆∞·ª£ng trong workspace
    for _, npc in pairs(workspace:GetDescendants()) do
        -- Ki·ªÉm tra xem ƒë·ªëi t∆∞·ª£ng c√≥ ph·∫£i l√† NPC (Model c√≥ Humanoid)
        if npc:IsA("Model") and npc ~= character and npc:FindFirstChild("Humanoid") and npc:FindFirstChild("HumanoidRootPart") then
            local dist = (npc.HumanoidRootPart.Position - root.Position).Magnitude
            -- Ki·ªÉm tra kho·∫£ng c√°ch gi·ªØa NPC v√† ng∆∞·ªùi ch∆°i
            if dist <= 10000000 then -- b√°n k√≠nh 50 studs (c√≥ th·ªÉ thay ƒë·ªïi)
                -- Gi·∫£m m√°u NPC xu·ªëng 0 (ch·∫øt)
                npc.Humanoid.Health = 0
            end
        end
    end
end

-- L√†m tr√≤n g√≥c
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 12)
corner.Parent = mainFrame

-- L√†m tr√≤n g√≥c
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 12)
corner.Parent = killBtn

-- Vi·ªÅn n·ªÅn m√†u t√≠m (vi·ªÅn n√∫t, kh√¥ng ph·∫£i ch·ªØ)
local stroke = Instance.new("UIStroke")
stroke.Color = Color3.fromRGB(170, 0, 255) -- M√†u t√≠m
stroke.Thickness = 2 -- ƒê·ªô d√†y vi·ªÅn
stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border -- Vi·ªÅn bao n√∫t
stroke.Parent = killBtn

-- Khi nh·∫•n n√∫t "Kill Nearby NPCs"
killBtn.MouseButton1Click:Connect(killNearbyNPCs)
   end,
})

local Button = Tab:CreateButton({
   Name = "Freeze Npc Near",
   Callback = function()
   local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local gui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
gui.Name = "FreezeNPCGui"
gui.ResetOnSpawn = false
gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

-- N√∫t GUI
local button = Instance.new("TextButton")
button.Size = UDim2.new(0, 160, 0, 50)
button.Position = UDim2.new(0.5, -80, 0.9, 0)
button.Text = "Freeze : Off"
button.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
button.TextColor3 = Color3.new(1, 1, 1)
button.Font = Enum.Font.SourceSansBold
button.TextScaled = true
button.Active = true
button.Draggable = true -- Cho ph√©p k√©o n√∫t
button.Parent = gui

local freezeEnabled = false
local radius = 100

-- ƒê√≥ng bƒÉng NPC g·∫ßn
local function toggleFreeze()
	freezeEnabled = not freezeEnabled

	if freezeEnabled then
		button.Text = "Freeze : On"
		button.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
	else
		button.Text = "Freeze : Off"
		button.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	end

	local rootPart = character:FindFirstChild("HumanoidRootPart")
	if not rootPart then return end

	for _, npc in pairs(workspace:GetDescendants()) do
		if npc:IsA("Model") and npc ~= character and npc:FindFirstChild("Humanoid") and npc:FindFirstChild("HumanoidRootPart") then
			local distance = (npc.HumanoidRootPart.Position - rootPart.Position).Magnitude
			if distance <= radius then
				npc.HumanoidRootPart.Anchored = freezeEnabled
				if freezeEnabled then
					npc.Humanoid:ChangeState(Enum.HumanoidStateType.Physics)
				else
					npc.Humanoid:ChangeState(Enum.HumanoidStateType.RunningNoPhysics)
				end
			end
		end
	end
end

-- L√†m tr√≤n g√≥c
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 12)
corner.Parent = button

-- Vi·ªÅn n·ªÅn m√†u t√≠m (vi·ªÅn n√∫t, kh√¥ng ph·∫£i ch·ªØ)
local stroke = Instance.new("UIStroke")
stroke.Color = Color3.fromRGB(170, 0, 255) -- M√†u t√≠m
stroke.Thickness = 2 -- ƒê·ªô d√†y vi·ªÅn
stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border -- Vi·ªÅn bao n√∫t
stroke.Parent = button

-- B·∫Øt s·ª± ki·ªán click
button.MouseButton1Click:Connect(toggleFreeze)
   end,
})

local Button = Tab:CreateButton({
   Name = "Select npc",
   Callback = function()
   loadstring(game:HttpGet("https://raw.githubusercontent.com/Kietba/Kietba/refs/heads/main/kibazkaiz.txt"))()
   end,
})

