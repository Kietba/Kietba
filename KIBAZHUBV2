local Button = Tab:CreateButton({
   Name = "Auto collect frozen soul",
   Callback = function()
   local player = game.Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local hrp = char:WaitForChild("HumanoidRootPart")

-- C√ÄI ƒê·∫∂T: T√™n Model v√† ClassName
local targetName = "Frozen Lock"
local targetClassName = "Model"

-- T·∫°o GUI Toggle
local gui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
gui.Name = "ToggleTeleportGui"
gui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
gui.ResetOnSpawn = false

local toggle = Instance.new("TextButton")
toggle.Size = UDim2.new(0, 50, 0, 50)
toggle.Position = UDim2.new(0.5, -100, 0.85, 0)
toggle.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
toggle.TextColor3 = Color3.fromRGB(255, 255, 255)
toggle.Text = "Collect Frozen: Off"
toggle.TextScaled = true
toggle.Parent = gui
toggle.Active = true
toggle.Draggable = true

-- H√†m l·∫•y CFrame c·ªßa Model
local function getModelCFrame(model)
	if model.PrimaryPart then
		return model:GetPrimaryPartCFrame()
	end
	local part = model:FindFirstChild("HumanoidRootPart") or model:FindFirstChildWhichIsA("BasePart")
	if part then
		return part.CFrame
	end
	return nil
end

-- Toggle loop
local running = false
local toggleConnection

toggle.MouseButton1Click:Connect(function()
running = not running
toggle.Text = running and "Collect: B·∫¨T" or "Collect: Off"
toggle.BackgroundColor3 = running and Color3.fromRGB(50, 220, 50) or Color3.fromRGB(255, 50, 50)

	if running then
		toggleConnection = task.spawn(function()
			while running do
				for _, obj in pairs(workspace:GetDescendants()) do
					if obj.ClassName == targetClassName and obj.Name == targetName then
						local cf = getModelCFrame(obj)
						if cf then
							hrp.CFrame = cf + Vector3.new(0, 3, 0)
							break
						end
					end
				end
				task.wait(1)
			end
		end)
	end
end)

-- G√≥c
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 15) -- S·ªë c√†ng l·ªõn th√¨ bo c√†ng tr√≤n
corner.Parent = toggle

-- Vi·ªÅn n·ªÅn m√†u t√≠m (vi·ªÅn n√∫t, kh√¥ng ph·∫£i ch·ªØ)
local stroke = Instance.new("UIStroke")
stroke.Color = Color3.fromRGB(170, 0, 255) -- M√†u t√≠m
stroke.Thickness = 2 -- ƒê·ªô d√†y vi·ªÅn
stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border -- Vi·ªÅn bao n√∫t
stroke.Parent = toggle

-- c√πng LocalScript

local runService = game:GetService("RunService")
local hue = 0

runService.RenderStepped:Connect(function(dt)
	hue = (hue + dt * 0.2) % 1
	toggle.TextColor3 = Color3.fromHSV(hue, 1, 1)
end)
   end,
})

