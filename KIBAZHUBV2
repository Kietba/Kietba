   Name = "Fe CrossHair Tap and Hold",
   Callback = function()
   loadstring(game:HttpGet("https://raw.githubusercontent.com/Kietba/Kietba/refs/heads/main/Fe-crosshair-kibazhub-game"))()
   end,
})

local Button = Tab:CreateButton({
   Name = "Tween player",
   Callback = function()
   -- D·ªãch v·ª• c·∫ßn d√πng
local TweenService = game:GetService("TweenService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- GUI ch√≠nh
local ScreenGui = Instance.new("ScreenGui", LocalPlayer:WaitForChild("PlayerGui"))
ScreenGui.Name = "TeleportGui"
ScreenGui.ResetOnSpawn = false

-- Frame ch√≠nh
local MainFrame = Instance.new("Frame", ScreenGui)
MainFrame.Size = UDim2.new(0, 250, 0, 240)
MainFrame.Position = UDim2.new(0.5, -125, 0.4, 0)
MainFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
MainFrame.Active = true
MainFrame.Draggable = true

-- TextBox
local TextBox = Instance.new("TextBox", MainFrame)
TextBox.Size = UDim2.new(1, -20, 0, 40)
TextBox.Position = UDim2.new(0, 10, 0, 10)
TextBox.PlaceholderText = "Enter Name Player..."
TextBox.Text = ""
TextBox.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
TextBox.TextColor3 = Color3.fromRGB(255, 255, 255)

-- N√∫t di chuy·ªÉn
local Button = Instance.new("TextButton", MainFrame)
Button.Size = UDim2.new(1, -20, 0, 40)
Button.Position = UDim2.new(0, 10, 0, 60)
Button.Text = "Tween To Player"
Button.BackgroundColor3 = Color3.fromRGB(30, 150, 80)
Button.TextColor3 = Color3.fromRGB(255, 255, 255)

-- N√∫t l∆∞u ƒëi·ªÉm
local SavePointButton = Instance.new("TextButton", MainFrame)
SavePointButton.Size = UDim2.new(1, -20, 0, 30)
SavePointButton.Position = UDim2.new(0, 10, 0, 110)
SavePointButton.Text = "Save Locate"
SavePointButton.BackgroundColor3 = Color3.fromRGB(150, 150, 30)
SavePointButton.TextColor3 = Color3.fromRGB(255, 255, 255)

-- N√∫t t·ªõi ƒëi·ªÉm ƒë√£ l∆∞u
local GoToSavedPointButton = Instance.new("TextButton", MainFrame)
GoToSavedPointButton.Size = UDim2.new(1, -20, 0, 30)
GoToSavedPointButton.Position = UDim2.new(0, 10, 0, 145)
GoToSavedPointButton.Text = "Tween To Select"
GoToSavedPointButton.BackgroundColor3 = Color3.fromRGB(150, 30, 150)
GoToSavedPointButton.TextColor3 = Color3.fromRGB(255, 255, 255)

-- N√∫t ">" m·ªü danh s√°ch
local ToggleListButton = Instance.new("TextButton", MainFrame)
ToggleListButton.Size = UDim2.new(0, 30, 0, 30)
ToggleListButton.Position = UDim2.new(1, -35, 0, 110)
ToggleListButton.Text = ">"
ToggleListButton.BackgroundColor3 = Color3.fromRGB(100, 100, 100)
ToggleListButton.TextColor3 = Color3.fromRGB(255, 255, 255)

-- Danh s√°ch ƒëi·ªÉm ƒë√£ l∆∞u
local SavedListFrame = Instance.new("ScrollingFrame", MainFrame)
SavedListFrame.Size = UDim2.new(0, 150, 0, 100)
SavedListFrame.Position = UDim2.new(1, 5, 0, 110)
SavedListFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
SavedListFrame.BorderSizePixel = 0
SavedListFrame.CanvasSize = UDim2.new(0, 0, 0, 0)
SavedListFrame.ScrollBarThickness = 4
SavedListFrame.Visible = false

-- Danh s√°ch ƒëi·ªÉm
local savedPoints = {}
local selectedPointName = nil

-- Hi·ªÉn th·ªã th√¥ng b√°o
local function showStatusMessage(message)
	local status = Instance.new("TextLabel", MainFrame)
	status.Size = UDim2.new(1, -20, 0, 20)
	status.Position = UDim2.new(0, 10, 1, -25)
	status.BackgroundTransparency = 1
	status.TextColor3 = Color3.new(1, 1, 1)
	status.TextScaled = true
	status.Text = message

	wait(1)
	status:Destroy()
end

-- C·∫≠p nh·∫≠t danh s√°ch ƒëi·ªÉm ƒë√£ l∆∞u
local function updateSavedList()
	SavedListFrame:ClearAllChildren()
	SavedListFrame.CanvasSize = UDim2.new(0, 0, 0, #savedPoints * 30)

	for i, pointData in ipairs(savedPoints) do
		local button = Instance.new("TextButton", SavedListFrame)
		button.Size = UDim2.new(1, -10, 0, 30)
		button.Position = UDim2.new(0, 5, 0, (i - 1) * 30)
		button.Text = pointData.name
		button.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
		button.TextColor3 = Color3.new(1, 1, 1)

		button.MouseButton1Click:Connect(function()
			selectedPointName = pointData.name
			showStatusMessage("Select: " .. selectedPointName)
		end)
	end
end

-- L∆∞u ƒëi·ªÉm hi·ªán t·∫°i
SavePointButton.MouseButton1Click:Connect(function()
	local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
	local hrp = character:WaitForChild("HumanoidRootPart")

	local pointName = "Locate " .. tostring(#savedPoints + 1)
	table.insert(savedPoints, {name = pointName, cframe = hrp.CFrame})
	showStatusMessage("Save " .. pointName)
	updateSavedList()
end)

-- Tween ƒë·∫øn ƒëi·ªÉm ƒë√£ ch·ªçn
GoToSavedPointButton.MouseButton1Click:Connect(function()
	if not selectedPointName then
		showStatusMessage("No save points yet")
		return
	end

	local point = nil
	for _, p in pairs(savedPoints) do
		if p.name == selectedPointName then
			point = p
			break
		end
	end

	if point then
		local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
		local hrp = character:WaitForChild("HumanoidRootPart")
		local tween = TweenService:Create(hrp, TweenInfo.new(1), {CFrame = point.cframe})
		tween:Play()
		showStatusMessage("ƒê√£ t·ªõi " .. selectedPointName)
	else
		showStatusMessage("Kh√¥ng t√¨m th·∫•y ƒëi·ªÉm ƒë√£ l∆∞u.")
	end
end)

-- L√†m tr√≤n g√≥c
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 12)
corner.Parent = TextBox

-- L√†m tr√≤n g√≥c
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 12)
corner.Parent = button

