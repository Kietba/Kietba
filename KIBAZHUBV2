local Button = Tab:CreateButton({
   Name = "Orbit target player",
   Callback = function()
   -- GUI setup
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
screenGui.Parent = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")
screenGui.ResetOnSpawn = false

local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 300, 0, 200)
frame.Position = UDim2.new(0.5, -150, 0.5, -100)
frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
frame.Parent = screenGui
frame.Active = true
frame.Draggable = true

local textBox = Instance.new("TextBox")
textBox.Size = UDim2.new(0, 200, 0, 40)
textBox.Position = UDim2.new(0, 50, 0, 10)
textBox.PlaceholderText = "Nh·∫≠p t√™n ng∆∞·ªùi ch∆°i..."
textBox.Parent = frame

local button = Instance.new("TextButton")
button.Size = UDim2.new(0, 200, 0, 40)
button.Position = UDim2.new(0, 50, 0, 60)
button.Text = "B·∫≠t Orbit"
button.Parent = frame

-- TextBox nh·ªè ch·ªânh t·ªëc ƒë·ªô
local speedBox = Instance.new("TextBox")
speedBox.Size = UDim2.new(0, 80, 0, 30)
speedBox.Position = UDim2.new(0, 110, 0, 110)
speedBox.Text = "1"
speedBox.PlaceholderText = "T·ªëc ƒë·ªô"
speedBox.ClearTextOnFocus = true
speedBox.Parent = frame

-- Bi·∫øn tr·∫°ng th√°i
local orbiting = false
local orbitConnection = nil
local speed = 1

-- T√¨m ng∆∞·ªùi ch∆°i
local function findPlayerByNameStart(namePart)
	for _, player in pairs(game.Players:GetPlayers()) do
		if player.Name:lower():sub(1, #namePart) == namePart:lower() then
			return player
		end
	end
	return nil
end

-- Orbit toggle
local function toggleOrbit()
	if orbiting then
		orbiting = false
		button.Text = "B·∫≠t Orbit"
		if orbitConnection then
			orbitConnection:Disconnect()
			orbitConnection = nil
		end
	else
		-- L·∫•y t·ªëc ƒë·ªô t·ª´ √¥ nh·∫≠p
		local inputSpeed = tonumber(speedBox.Text)
		if inputSpeed and inputSpeed > 0 then
			speed = inputSpeed
		else
			speed = 1
			speedBox.Text = "1"
		end

		local targetName = textBox.Text
		local targetPlayer = findPlayerByNameStart(targetName)
		if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
			local targetHRP = targetPlayer.Character.HumanoidRootPart
			local myChar = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
			local angle = 0
			orbiting = true
			button.Text = "T·∫Øt Orbit"

			orbitConnection = game:GetService("RunService").RenderStepped:Connect(function()
				if not orbiting or not myChar or not myChar.PrimaryPart or not targetHRP then return end
				angle += speed
				local radius = 5
				local orbitPos = targetHRP.Position + Vector3.new(math.cos(math.rad(angle)) * radius, 0, math.sin(math.rad(angle)) * radius)
				myChar:SetPrimaryPartCFrame(CFrame.new(orbitPos, targetHRP.Position))
			end)
		else
			warn("Kh√¥ng t√¨m th·∫•y ng∆∞·ªùi ch∆°i ph√π h·ª£p")
		end
	end
end

-- G·∫Øn s·ª± ki·ªán
button.MouseButton1Click:Connect(toggleOrbit)
   end,
})

local Button = Tab:CreateButton({
   Name = "Anti fling",
   Callback = function()
   -- Smart Anti-Fling: ch·ªâ ƒë·ª©ng im khi b·ªã fling b·∫•t th∆∞·ªùng
-- ƒê·∫∑t LocalScript v√†o StarterPlayerScripts

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local root
local humanoid
local safeCFrame

RunService.RenderStepped:Connect(function()
	local character = player.Character
	if not character then return end

	root = character:FindFirstChild("HumanoidRootPart")
	humanoid = character:FindFirstChildOfClass("Humanoid")

	if not root or not humanoid then return end

	-- Ghi nh·ªõ v·ªã tr√≠ an to√†n n·∫øu t·ªëc ƒë·ªô b√¨nh th∆∞·ªùng
	if root.Velocity.Magnitude < 50 and root.RotVelocity.Magnitude < 30 then
		safeCFrame = root.CFrame
	else
		-- N·∫øu b·ªã fling b·∫•t th∆∞·ªùng, ƒë·ª©ng im t·∫°i v·ªã tr√≠ an to√†n
		if safeCFrame then
			root.CFrame = safeCFrame
			root.Velocity = Vector3.zero
			root.RotVelocity = Vector3.zero
			humanoid:ChangeState(Enum.HumanoidStateType.GettingUp)
		end
	end
end)
   end,
})

