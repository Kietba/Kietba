    loadstring(game:HttpGet("https://raw.githubusercontent.com/ahmadsgamer2/Speed-Hub-X/main/SpeedHubX", true))()
end)
tab1.newButton("Chạy cực nhanh, "", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Kietba/Kietba/main/Speed 0.9"))()
