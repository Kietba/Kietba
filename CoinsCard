-- Load the DrRay library from the GitHub repository Library
local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()

-- Create a new window and set its title and theme
local window = DrRayLibrary:Load("Kiệt Bá", "Default")

-- Create the first tab with an image ID
local tab1 = DrRayLibrary.newTab("Blox fruit", "ImageIdHere")

-- Add elements to the first tab
tab1.newLabel("Script của blox fruit")
tab1.newButton("Redz hub", "Không cần chiều khoá", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/REDzHUB/BloxFruits/main/redz9999"))()
end)
tab1.newButton("Azure", "Thời gian load : 10 giây", function()
    getgenv().FixCrash = true
getgenv().FixCrash2 = true
loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/3b2169cf53bc6104dabe8e19562e5cc2.lua"))()
end)
tab1.newButton("Auto săn bounty v1", "Xịn nhưng dễ chết", function()
    loadstring(game:HttpGet("https://scriptblox.com/raw/Blox-Fruits-Auto-Bounty-11164"))()
end)
tab1.newButton("Auto săn bounty v2", "Xịn và nên dùng", function()
    loadstring(game:HttpGet("https://scriptblox.com/raw/Blox-Fruits-auto-bounty-script-12154"))()
end)
tab1.newButton("Maru hub v1", "Không cần chiều khoá", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/joi-droid/MaruHubBF/main/GOHANSSJ3'))()
end)
tab1.newButton("Maru hub v2", "Không cần chiều khoá", function()
